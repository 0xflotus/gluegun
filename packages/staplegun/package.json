{
  "name": "staplegun",
  "version": "0.1.1",
  "description": "Tack stuff on to your codebase.",
  "main": "src/index.js",
  "bin": {
    "staplegun": "./dist/staplegun"
  },
  "scripts": {
    "test": "node --harmony_async_await node_modules/.bin/ava",
    "watch": "node --harmony node_modules/.bin/ava --watch",
    "coverage": "nyc ava",
    "start": "node --harmony_async_await src/index.js",
    "build": "rollup -c && mv dist/staplegun dist/staplegun.tmp && echo '#!/usr/bin/env node\n' > ./dist/staplegun && cat dist/staplegun.tmp >> ./dist/staplegun && chmod +x ./dist/staplegun && rm dist/staplegun.tmp",
    "lint": "standard"
  },
  "author": {
    "name": "Steve Kellock",
    "email": "steve@kellock.ca",
    "url": "https://github.com/skellock/staplegun"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "types.js",
    "README.md"
  ],
  "license": "MIT",
  "dependencies": {
    "ascii-table": "^0.0.9",
    "autobind-decorator": "^1.3.4",
    "colors": "^1.1.2",
    "fs-jetpack": "^0.10.3",
    "inquirer": "^1.2.3",
    "lodash": "^4.17.2",
    "minimist": "^1.2.0",
    "nunjucks": "^3.0.0",
    "ramda": "^0.22.1",
    "ramdasauce": "^1.1.1",
    "shelljs": "^0.7.5",
    "toml": "^2.3.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-eslint": "^7.1.1",
    "flow-bin": "^0.36.0",
    "nyc": "^10.0.0",
    "rollup": "^0.36.4",
    "standard": "^8.6.0"
  },
  "ava": {},
  "standard": {
    "parser": "babel-eslint"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
