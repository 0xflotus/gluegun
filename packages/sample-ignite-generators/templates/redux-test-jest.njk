import Actions, { reducer, INITIAL_STATE } from '../../App/Redux/{{ props.name }}Redux'

it('attempt', () => {
  const state = reducer(INITIAL_STATE, Actions.{{ props.camelCase }}Request('data'))

  expect(state.fetching).toBe(true)
})

it('success', () => {
  const state = reducer(INITIAL_STATE, Actions.{{ props.camelCase }}Success('hi'))

  expect(state.payload).toBe('hi')
})

it('failure', () => {
  const state = reducer(INITIAL_STATE, Actions.{{ props.camelCase }}Failure())

  expect(state.fetching).toBe(false)
  expect(state.error).toBe(true)
})
